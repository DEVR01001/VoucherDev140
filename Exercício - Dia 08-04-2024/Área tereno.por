programa {
  funcao inicio() {
          inteiro base, altura, area 
          escreva("Bem vindo ao calcule a área do retângulo!! \n")
          escreva("Digite a base do terreno: \n" )
          leia(base)
          escreva("Digite a altura do terreno: \n")
          leia(altura)
          area = base*altura
          escreva("A área desse terro é ", area)

  }
}
