programa {
  funcao inicio() {
    
          inteiro numero, positivo, negativo
          escreva("Digite um numero inteiro: ")
          leia(numero)
          se (numero >= 0) {
            escreva("\nSeu numero é ", numero)
            escreva("\nEle é positivo!!!")
          }
          senao {
            escreva("\nSeu numero é ", numero)
            escreva("\nEle é negativo!! ")
            
                      }



  }
}
