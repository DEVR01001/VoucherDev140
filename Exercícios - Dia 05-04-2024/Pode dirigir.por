programa {
  funcao inicio() {
    inteiro idade
    escreva("Bem vindo ao código que diz se você pode ou não dirigir!! \n")
    escreva("Digite sua idade: ")
    leia(idade)
    se (idade < 18)
    {
      escreva("Calculando... \n")
      escreva("Você não pode dirigir!")
      
    }
    senao
    {
      escreva("Calculando... \n")
      escreva("Você pode dirigir!")
  
    }





  }
}
